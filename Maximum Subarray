
class Solution {
    public int maxSubArray(int[] nums) {
        int max = nums[0]; // Initialize maxSum with the first element
        int currsum = nums[0]; // Initialize currentSum with the first element

        for (int i = 1; i < nums.length; i++) {
            
            currsum = Math.max(nums[i], currsum + nums[i]);
            
            max = Math.max(max, currsum);
        }

        return max;


        
    }
}
